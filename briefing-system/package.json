{
  "name": "pulsedaily-briefing-system",
  "version": "1.0.0",
  "description": "PulseX Daily Briefing System - Complete production-ready implementation",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "migrate": "npm run build && node dist/database/migrate.js",
    "migrate:prod": "node dist/database/migrate.js",
    "seed": "npm run build && node dist/database/seed.js",
    "jobs:worker": "ts-node src/jobs/worker.ts",
    "jobs:scheduler": "ts-node src/jobs/scheduler.ts",
    "docker:build": "docker build -t pulsedaily-briefing .",
    "docker:run": "docker-compose up -d"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.9.1",
    "bull": "^4.11.3",
    "@bull-board/api": "^5.8.0",
    "@bull-board/express": "^5.8.0",
    "@bull-board/ui": "^5.8.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17",
    "typeorm-extension": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "winston-elasticsearch": "^0.17.3",
    "moment-timezone": "^0.5.43",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "ml-matrix": "^6.10.9",
    "natural": "^6.8.0",
    "sentiment": "^5.0.2",
    "prom-client": "^15.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "newrelic": "^11.1.0",
    "@sentry/node": "^7.84.0",
    "@sentry/tracing": "^7.84.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/compression": "^1.7.5",
    "@types/pg": "^8.10.7",
    "@types/jest": "^29.5.7",
    "@types/lodash": "^4.14.201",
    "@types/uuid": "^9.0.6",
    "@types/node-cron": "^3.0.11",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/natural": "^5.1.5",
    "@types/sentiment": "^5.0.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "daily briefing",
    "ai content",
    "personalization",
    "news aggregation",
    "cryptocurrency",
    "typescript",
    "nodejs",
    "express"
  ],
  "author": "PulseX Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pulsex/briefing-system.git"
  }
